{
 "compilerOptions": {
  "target": "es5",
   "module": "commonjs",
   "outDir": "./functions/functions",
   "rootDir": "./src",
   "strict": true,
   "noImplicitAny": true,
   "strictNullChecks": false,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "forceConsistentCasingInFileNames": true
},
"include": ["src/**/*"],
 "exclude": [
   "node_modules",
   "**/*.spec.ts"
  ]
}
